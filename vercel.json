{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "functions": {
        "api/logs.ts": {
          "maxDuration": 10
        }
      }
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
