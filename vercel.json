{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/logs.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
